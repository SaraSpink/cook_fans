<%= render "layouts/errors", :object => @piece %>

<%= form_for [@piece] do |f| %>
  <%= f.label :title %>
  <%= f.text_field :title %>
  <%= f.label :artist %>
  <%= f.text_field :artist %>
  <%= f.label :created %>
  <%= f.date_field :created %>
  <%= f.label :image %>
  <%= f.text_field :image %>
  <%= f.submit %>
<% end %>
